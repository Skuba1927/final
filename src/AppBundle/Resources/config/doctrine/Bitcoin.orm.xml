<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Bitcoin" table="bitcoin">
    <id name="date" type="datetime" column="date">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sell" type="float" column="sell" precision="10" scale="0" nullable="false"/>
    <field name="buy" type="float" column="buy" precision="10" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
